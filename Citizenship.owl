    <!-- http://www.semanticweb.org/mjlaali/ontologies/2013/10/Citizenship#ECA -->

    <owl:Class rdf:about="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Citizenship#ECA">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Citizenship#FSWRequirment"/>
        <rdfs:comment>include either a completed Canadian educational credential or a completed foreign educational credential with an Educational Credential Assessment (ECA) from an approved organization, 
http://www.cic.gc.ca/english/immigrate/skilled/apply-who-instructions.asp?expand=arranged#arranged
</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mjlaali/ontologies/2013/10/Citizenship#FSW -->

    <owl:Class rdf:about="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Citizenship#FSW">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Citizenship#ECA"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Citizenship#FSWLanguageRequirment"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Citizenship#OneYearWorkExperiment"/>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Citizenship#FSWBasicRequirment"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Citizenship#HaveOfferForEmployment"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Citizenship#PhDStudent"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Common#PermanentResident"/>
        <rdfs:comment>Federal skilled workers (FSW) </rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mjlaali/ontologies/2013/10/Citizenship#FSWBasicRequirment -->

    <owl:Class rdf:about="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Citizenship#FSWBasicRequirment">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Citizenship#FSWRequirment"/>
        <rdfs:comment>have one year of continuous full-time (or an equal amount of continuous part-time) paid work experience
in one of the eligible occupations within the last ten years, 
http://www.cic.gc.ca/english/immigrate/skilled/apply-who-instructions.asp?expand=arranged#arranged</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mjlaali/ontologies/2013/10/Citizenship#FSWLanguageRequirment -->

    <owl:Class rdf:about="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Citizenship#FSWLanguageRequirment">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Citizenship#FSWRequirment"/>
        <rdfs:comment>include the results of your official language test showing that you meet the minimum language threshold (Canadian Language Benchmark 7) in all four language skill areas (speaking, listening, reading and writing) when you apply,</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mjlaali/ontologies/2013/10/Citizenship#FSWRequirment -->

