        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Citizenship#FSWRequirment"/>
        <rdfs:comment>have a valid offer of arranged employment, 
http://www.cic.gc.ca/english/immigrate/skilled/apply-who-instructions.asp?expand=arranged#arranged</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mjlaali/ontologies/2013/10/Citizenship#LanguageAbilitiesForCitizenship -->

    <owl:Class rdf:about="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Citizenship#LanguageAbilitiesForCitizenship">
        <owl:equivalentClass>
