    </owl:Class>
    


    <!-- http://www.semanticweb.org/mjlaali/ontologies/2013/10/PR#RegularSelection -->

    <owl:Class rdf:about="http://www.semanticweb.org/mjlaali/ontologies/2013/10/PR#RegularSelection">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="&Activity;doActivity"/>
                                <owl:someValuesFrom>
                                    <owl:Class>
                                        <owl:intersectionOf rdf:parseType="Collection">
                                            <rdf:Description rdf:about="&Activity;Studying"/>
                                            <owl:Restriction>
                                                <owl:onProperty rdf:resource="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Person#locatedIn"/>
                                                <owl:hasValue rdf:resource="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Country#Quebec"/>
                                            </owl:Restriction>
                                            <owl:Restriction>
                                                <owl:onProperty rdf:resource="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Person#activityLength"/>
                                                <owl:someValuesFrom>
                                                    <rdfs:Datatype>
                                                        <owl:onDatatype rdf:resource="&xsd;nonNegativeInteger"/>
                                                        <owl:withRestrictions rdf:parseType="Collection">
                                                            <rdf:Description>
                                                                <xsd:minInclusive rdf:datatype="&xsd;integer">365</xsd:minInclusive>
                                                            </rdf:Description>
                                                        </owl:withRestrictions>
                                                    </rdfs:Datatype>
                                                </owl:someValuesFrom>
                                            </owl:Restriction>
                                        </owl:intersectionOf>
                                    </owl:Class>
                                </owl:someValuesFrom>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="&Activity;doActivity"/>
                                <owl:someValuesFrom>
                                    <owl:Class>
                                        <owl:intersectionOf rdf:parseType="Collection">
                                            <rdf:Description rdf:about="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Person#Living"/>
                                            <owl:Restriction>
                                                <owl:onProperty rdf:resource="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Person#locatedIn"/>
                                                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Country#OutsideCanada"/>
                                            </owl:Restriction>
                                        </owl:intersectionOf>
                                    </owl:Class>
                                </owl:someValuesFrom>
                            </owl:Restriction>
                        </owl:unionOf>
                    </owl:Class>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/mjlaali/ontologies/2013/10/PR#meetCSQRequirment"/>
                        <owl:hasValue rdf:datatype="&xsd;boolean">true</owl:hasValue>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mjlaali/ontologies/2013/10/PR#CSQ"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mjlaali/ontologies/2013/10/PR#ValidWorkExperienceForFSW -->

    <owl:Class rdf:about="http://www.semanticweb.org/mjlaali/ontologies/2013/10/PR#ValidWorkExperienceForFSW">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&Activity;Working"/>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="&Activity;hasSkillLevel"/>
                                <owl:hasValue rdf:datatype="&xsd;string">0</owl:hasValue>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="&Activity;hasSkillLevel"/>
                                <owl:hasValue rdf:datatype="&xsd;string">A</owl:hasValue>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="&Activity;hasSkillLevel"/>
                                <owl:hasValue rdf:datatype="&xsd;string">B</owl:hasValue>
                            </owl:Restriction>
                        </owl:unionOf>
                    </owl:Class>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Person#activityLength"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="&xsd;decimal"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:minExclusive rdf:datatype="&xsd;integer">365</xsd:minExclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&Activity;Working"/>
        <rdfs:comment>Valid work experience for Fedoral Skill Worker (FSW)
</rdfs:comment>
    </owl:Class>
    


