    </owl:Class>
    


    <!-- http://www.semanticweb.org/mjlaali/ontologies/2013/10/PR#Refugees -->

    <owl:Class rdf:about="http://www.semanticweb.org/mjlaali/ontologies/2013/10/PR#Refugees">
