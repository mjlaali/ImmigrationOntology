    </owl:Class>
    


    <!-- http://www.semanticweb.org/mjlaali/ontologies/2013/10/PR#OtherPR -->

    <owl:Class rdf:about="http://www.semanticweb.org/mjlaali/ontologies/2013/10/PR#OtherPR">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mjlaali/ontologies/2013/10/PR#PR"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mjlaali/ontologies/2013/10/PR#PEQ -->

    <owl:Class rdf:about="http://www.semanticweb.org/mjlaali/ontologies/2013/10/PR#PEQ">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Common#tookATest"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Test#B2SpeakingListening"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Person#didActivity"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <owl:Class>
                                        <owl:unionOf rdf:parseType="Collection">
                                            <rdf:Description rdf:about="&Activity;Bachelor"/>
                                            <rdf:Description rdf:about="&Activity;Master"/>
                                            <rdf:Description rdf:about="&Activity;PhD"/>
                                        </owl:unionOf>
                                    </owl:Class>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Person#locatedIn"/>
                                        <owl:hasValue rdf:resource="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Country#Quebec"/>
                                    </owl:Restriction>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mjlaali/ontologies/2013/10/PR#CSQ"/>
