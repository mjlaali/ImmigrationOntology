                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.semanticweb.org/mjlaali/ontologies/2013/10/Activity#hasSkillLevel"/>
                                <owl:hasValue rdf:datatype="&xsd;string">B</owl:hasValue>
                            </owl:Restriction>
                        </owl:unionOf>
                    </owl:Class>
